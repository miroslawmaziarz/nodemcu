require 'net/https'

uri = URI.parse("https://script.google.com/macros/s/AKfycbxez4AeeZmGxF3QG7RSnjuXRUn3LAdbuk6wnzOeyc7aknCQOs9h2CNB0JIDN014JoYZ1Q/exec")
